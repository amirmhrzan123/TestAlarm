<?xml version="1.0" encoding="utf-8"?>

<layout xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>



    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        >

        <androidx.core.widget.NestedScrollView
            android:id="@+id/nestedScrollView"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_width="0dp"
            android:layout_height="0dp"
           >

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">


                <RelativeLayout
                    android:id="@+id/rlAccount"
                    android:layout_width="0dp"
                    android:paddingStart="16dp"
                    android:paddingEnd="16dp"
                    android:foreground="?attr/selectableItemBackground"
                    android:layout_height="wrap_content"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textSize="18sp"
                        android:layout_centerVertical="true"
                        android:layout_marginEnd="8dp"
                        android:layout_toStartOf="@id/imageView3"
                        android:padding="16dp"
                        android:text="My Profile" />

                    <ImageView
                        android:id="@+id/imageView3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_marginTop="16dp"
                        app:srcCompat="@drawable/ic_arrow_right" />

                    <include
                        layout="@layout/view_divider"
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_below="@id/imageView3"
                        android:layout_marginTop="16dp" />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/rlRentalLedger"
                    android:foreground="?attr/selectableItemBackground"
                    android:layout_width="0dp"
                    android:paddingStart="16dp"
                    android:paddingEnd="16dp"
                    android:layout_height="wrap_content"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/rlAccount">

                    <TextView
                        android:id="@+id/tvSmsContent"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginEnd="8dp"
                        android:textSize="18sp"
                        android:layout_toStartOf="@id/imageView5"
                        android:padding="8dp"
                        android:text="Change sms message content" />

                    <ImageView
                        android:id="@+id/imageView5"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_marginTop="16dp"
                        app:srcCompat="@drawable/ic_arrow_right" />

                    <include
                        layout="@layout/view_divider"
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_below="@id/tvSmsContent"
                        android:layout_marginTop="16dp" />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/rlMaintenance"
                    android:foreground="?attr/selectableItemBackground"
                    android:layout_width="0dp"
                    android:paddingStart="16dp"
                    android:paddingEnd="16dp"
                    android:layout_height="wrap_content"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/rlRentalLedger">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:textSize="18sp"
                        android:layout_marginEnd="8dp"
                        android:layout_toStartOf="@id/imageView6"
                        android:padding="16dp"
                        android:text="Notify that you are safe"
                        />

                    <ImageView
                        android:id="@+id/imageView6"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_marginTop="16dp"
                        app:srcCompat="@drawable/ic_arrow_right" />

                    <include
                        layout="@layout/view_divider"
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_below="@id/imageView6"
                        android:layout_marginTop="16dp" />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/rlRecommendATenant"
                    android:foreground="?attr/selectableItemBackground"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:paddingStart="16dp"
                    android:paddingEnd="16dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/rlMaintenance">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:textSize="18sp"
                        android:layout_marginEnd="8dp"
                        android:layout_toStartOf="@id/imageView7"
                        android:padding="16dp"
                        android:text="Recommend SI friend" />

                    <ImageView
                        android:id="@+id/imageView7"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_marginTop="16dp"
                        app:srcCompat="@drawable/ic_arrow_right" />

                    <include
                        layout="@layout/view_divider"
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_below="@id/imageView7"
                        android:layout_marginTop="16dp" />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/rlSwitchLandLordProfile"
                    android:foreground="?attr/selectableItemBackground"
                    android:layout_width="0dp"
                    android:paddingStart="16dp"
                    android:paddingEnd="16dp"
                    android:layout_height="wrap_content"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/rlRecommendATenant">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:textSize="18sp"
                        android:layout_marginEnd="8dp"
                        android:layout_toStartOf="@id/imageView8"
                        android:padding="16dp"
                        android:text="About us" />

                    <ImageView
                        android:id="@+id/imageView8"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_marginTop="16dp"
                        app:srcCompat="@drawable/ic_arrow_right" />

                    <include
                        layout="@layout/view_divider"
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_below="@id/imageView8"
                        android:layout_marginTop="16dp" />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/rLsubscription"
                    android:foreground="?attr/selectableItemBackground"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:paddingStart="16dp"
                    android:paddingEnd="16dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/rlSwitchLandLordProfile">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginEnd="8dp"
                        android:textSize="18sp"
                        android:layout_toStartOf="@id/imageView10"
                        android:padding="16dp"
                        android:text="Logout" />

                    <ImageView
                        android:id="@+id/imageView10"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_marginTop="16dp"
                        app:srcCompat="@drawable/ic_arrow_right" />

                    <include
                        layout="@layout/view_divider"
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_below="@id/imageView10"
                        android:layout_marginTop="16dp" />
                </RelativeLayout>



                <androidx.constraintlayout.widget.Guideline
                    android:id="@+id/guidelineLeft_16dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    app:layout_constraintGuide_begin="16dp" />

                <androidx.constraintlayout.widget.Guideline
                    android:id="@+id/guidelineRight_16dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    app:layout_constraintGuide_end="16dp" />

            </androidx.constraintlayout.widget.ConstraintLayout>




        </androidx.core.widget.NestedScrollView>



    </androidx.constraintlayout.widget.ConstraintLayout>




</layout>


